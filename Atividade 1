    <p id="mensagem">Texto original</p>
    <br>
    <button id="bot達o">clique</button>
    <script>
        var msg = document.getElementById ("mensagem")
        var bot達o = document.getElementById ("bot達o")

        bot達o.addEventListener ("click", clicou)

        function clicou(){
            msg.innerHTML = "Texto alterado com sucesso!"
        }
    </script>
